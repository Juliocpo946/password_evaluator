          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/v1/password/evaluate",
              "host": ["{{base_url}}"],
              "path": ["api", "v1", "password", "evaluate"]
            },
            "description": "Contraseña común en diccionario con patrón numérico simple"
          },
          "response": [
            {
              "name": "200 - Débil",
              "status": "OK",
              "code": 200,
